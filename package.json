{
   "name": "app",
   "private": true,
   "version": "2.7.2",
   "description": "a Sails application",
   "keywords": [],
   "dependencies": {
      "@digiserve/ab-utils": "github:CruGlobal/ab-utils#1.10.2",
      "@sailshq/connect-redis": "^6.1.3",
      "@sailshq/lodash": "^3.10.3",
      "@sailshq/socket.io-redis": "^6.1.2",
      "@sentry/node": "^7.70.0",
      "async": "^3.2.0",
      "cote": "^1.2.0",
      "passport": "^0.7.0",
      "passport-cas2": "0.0.12",
      "passport-local": "^1.0.0",
      "passport-openidconnect": "^0.1.1",
      "passport-trusted-header": "^1.1.0",
      "passport-unique-token": "^3.0.0",
      "prom-client": "^15.1.3",
      "sails": "^1.5.14",
      "sails-hook-orm": "^4.0.0",
      "sails-hook-sockets": "^3.0.2",
      "sails-mysql": "^3.0.1",
      "semver": "^7.7.3",
      "shelljs": "^0.10.0"
   },
   "devDependencies": {
      "apidoc-markdown": "^7.2.5",
      "eslint": "^9.26.0",
      "eslint-config-prettier": "^10.1.8",
      "eslint-plugin-prettier": "^5.4.0",
      "prettier": "^3.0.0",
      "run-script-os": "^1.1.5",
      "@eslint/js": "^9.15.0",
      "globals": "^16.0.0",
      "nodemon": "^3.1.11"
   },
   "scripts": {
      "start": "NODE_ENV=production node app.js",
      "test": "npm run lint && npm run custom-tests && echo 'Done.'",
      "lint": "eslint . --fix --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your .js files look good.'",
      "dev": "node_modules/.bin/nodemon --inspect=0.0.0.0:9229 app.js",
      "custom-tests": "echo \"(No other custom tests yet.)\" && echo",
      "devBuild": "run-script-os",
      "devInstall": "run-script-os",
      "devBuild:darwin:linux": "docker run -it --mount type=bind,source=\"$(pwd)\",target=/app -w /app node npm update",
      "devInstall:darwin:linux": "docker run -it -p 1337:1337 --mount type=bind,source=\"$(pwd)\",target=/app -w /app node bash",
      "devBuild:win32": "docker run -it --mount type=bind,source=%cd%,target=/app -w /app node npm update",
      "devInstall:win32": "docker run -it -p 1337:1337 --mount type=bind,source=%cd%,target=/app -w /app node bash",
      "doc": "apidoc-markdown -i api/controllers -o README.md -t README.ejs"
   },
   "main": "app.js",
   "repository": {
      "type": "git",
      "url": "git://github.com/dad/app.git"
   },
   "author": "Johnny Hausman",
   "license": "",
   "overrides": {
      "glob": "^13.0.0",
      "acorn-import-assertions": "npm:acorn-import-attributes@^1.9.5",
      "debug": "^4.3.1",
      "qs": "^6.14.1",
      "lodash": "^4.17.21",
      "css-what": "^2.1.3",
      "nth-check": "^2.0.1"
   }
}
